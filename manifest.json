{
    "manifest_version": 2,
    "name": "nikineko",
    "version": "1.0",
    "description": "Adds the jNeko to every page on your browser.",
    "icons": 
          {
          "48": "icon.png"
          },
    "content_scripts": 
          [
              {
              "matches": ["<all_urls>"],
              "js": ["source.js"]
              }
          ],
        "background": {
        "scripts": ["oneko.js"]
        },
        "browser_action": {
            "default_icon": "icon.png"
        }
  }